def maxArea(heights):
    l, r = 0, len(heights) - 1
    res = 0

    while l < r:
        area = (r - l) * min(heights[l], heights[r])
        res = max(res, area)
        if heights[l] < heights[r]:
            l += 1
        else:
            r -= 1

    return res

print(maxArea([1,7,2,5,4,7,3,6]))